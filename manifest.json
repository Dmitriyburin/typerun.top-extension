{
    "name": "TypeRun",
    "description": "Change style typerun",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
        "matches": ["https://typerun.top/*"],
         "js": ["popup.js"],
         "run_at": "document_end"
        }
       ], 
    "action": {
        "default_icon": {
          "16": "/images/get_started16.png",
          "32": "/images/get_started32.png",
          "48": "/images/get_started48.png",
          "128": "/images/get_started128.png"
        }
      },
      "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      }
}
